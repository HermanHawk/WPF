<Window x:Class="WpfAppMNNMDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppMNNMDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Text="{Binding Name}" Margin="5" Height="30" Width="100"/>
            <TextBox Text="{Binding Title}" Margin="5" Height="30" Width="100"/>
            <Button Content="command" Margin="5" Height="30" Width="100" Command="{Binding MyCommand}"/>
        </StackPanel>
    </Grid>
</Window>
